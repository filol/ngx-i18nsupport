{
  "extends": "../tsconfig.lib.json",
  "compilerOptions": {
    "outDir": "../../../dist/ngx-i18nsupport-lib/src",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "types": [
      "jasmine",
      "node"
    ]
  },
  "include": [
    "api/**/*",
    "impl/**/*"
  ],
  "exclude": [
  ]
}
